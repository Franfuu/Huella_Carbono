<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.Franfuu.view.InicioSesionUsuarioController">
    <children>
        <TextField fx:id="emailField" layoutX="200.0" layoutY="100.0" promptText="Email" />
        <PasswordField fx:id="passwordField" layoutX="200.0" layoutY="150.0" promptText="Password" />
        <Button fx:id="loginButton" layoutX="210.0" layoutY="197.0" onAction="#handleLogin" text="Login" />

        <Button fx:id="welcomeButton" layoutX="200.0" layoutY="250.0" onAction="#setWelcomeButton" text="Welcome" />

    </children>
</AnchorPane>
