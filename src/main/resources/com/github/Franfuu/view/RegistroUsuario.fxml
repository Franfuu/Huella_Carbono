<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.Franfuu.view.RegistroUsuarioController">
    <children>
        <TextField fx:id="nameField" layoutX="200.0" layoutY="50.0" promptText="Name" />
        <TextField fx:id="emailField" layoutX="200.0" layoutY="100.0" promptText="Email" />
        <PasswordField fx:id="passwordField" layoutX="200.0" layoutY="150.0" promptText="Password" />
        <Button fx:id="registerButton" layoutX="200.0" layoutY="200.0" text="Register" />
        <Button fx:id="welcomeButton" layoutX="200.0" layoutY="250.0" onAction="#setWelcomeButton" text="Welcome" />
        <Text fx:id="registerMessage" layoutX="200.0" layoutY="300.0" />
    </children>
</AnchorPane>
